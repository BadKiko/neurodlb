# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Local Telegram Bot API Server (для файлов >50MB)
# TELEGRAM_BOT_API_URL=http://localhost:8081

# Telegram API credentials for local server (если используется локальный сервер)
# TELEGRAM_API_ID=your_telegram_api_id
# TELEGRAM_API_HASH=your_telegram_api_hash

# Mistral API Configuration
MISTRAL_API_KEY=your_mistral_api_key_here

# Proxy Configuration (опционально)
# Поддерживаются HTTP, HTTPS и SOCKS прокси
# HTTPS_PROXY=http://username:password@proxy.example.com:8080
# HTTP_PROXY=http://username:password@proxy.example.com:8080
# SOCKS_PROXY=socks5://username:password@proxy.example.com:1080

# Примеры прокси:
# HTTPS_PROXY=http://uLVqJ4Tc:HQDZhU67@156.246.238.211:64680
# HTTP_PROXY=http://user:pass@proxy.example.com:3128
# SOCKS_PROXY=socks5://user:pass@proxy.example.com:1080

# Video Processing Settings
MAX_VIDEO_DURATION=600
MAX_FILE_SIZE_MB=2048

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=bot.log

# Temporary Files Directory
TEMP_DIR=temp
